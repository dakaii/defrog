# LLM Configuration (OpenAI, DeepSeek, Qwen, etc.)
# For OpenAI (default):
OPENAI_API_KEY=your_openai_api_key_here
#LLM_BASE_URL=  # Leave empty for OpenAI

# For DeepSeek:
#LLM_API_KEY=your_deepseek_api_key
#LLM_BASE_URL=https://api.deepseek.com/v1
#LLM_MODEL=deepseek-chat

# For Qwen (via Dashscope):
#LLM_API_KEY=your_dashscope_api_key  
#LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
#LLM_MODEL=qwen-max

# Model Configuration
LLM_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small

# Use separate keys/URLs for embeddings if needed
#EMBEDDING_API_KEY=your_embedding_api_key
#EMBEDDING_BASE_URL=https://api.openai.com/v1

# Database Configuration (POSTGRES_HOST=postgres when using Docker Compose)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=defrog
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres